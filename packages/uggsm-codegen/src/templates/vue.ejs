          type: [
            <% for (const propType in props[prop].type) { %>
              <%- props[prop].type[propType] %>,
            <% } %>
          ],
          <% if (props[prop].default) { %>
            default: <%- props[prop].default %>,
          <% } %>
        },
      <% } %>
    },
    data: function () {
      return <%- JSON.stringify(data ? data : {}) %>
    },
    computed: {
      <% for (let i = 0; i < computed.length; i++) { %>
        <% if (computed[i].withSetter) { %>
          <%= computed[i].value %>: {
            get: function() {

            },
            set: function(value) {

            }
          },
        <% } else {%>
          <%= computed[i].value %>() {},
        <% } %>
      <% } %>
    },
    methods: {
      <% for (let i = 0; i < methods.length; i++) { %>
        <%= methods[i].value %>() {}
      <% } %>
    }
  }
</script>
